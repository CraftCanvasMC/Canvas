+        if (number >= 1.0) {
+            return Math.round(number * 100.0) / 100.0;
+        }
+
+        double precision = 0.1;
+        while (number < precision) {
+            precision /= 10;
+        }
+
+        return Math.round(number / precision) * precision;
+    }
