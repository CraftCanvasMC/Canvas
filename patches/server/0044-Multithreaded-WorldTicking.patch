+package me.dueris.canvas.thread.region;
+
+import net.minecraft.world.level.ChunkPos;
+
+public record BubbleRegion(ChunkPos[][] chunks, long id) {
+
+    public int size() {
+        return chunks.length * chunks[0].length;
+    }
+
+    public ChunkPos getCenterChunkPos() {
+        long totalX = 0;
+        long totalZ = 0;
+        int count = 0;
+
+        for (ChunkPos[] row : chunks) {
+            for (ChunkPos chunk : row) {
+                if (chunk != null) {
+                    totalX += chunk.x();
+                    totalZ += chunk.z();
+                    count++;
+                }
+            }
+        }
+
+        if (count == 0) {
+            return null;
+        }
+
+        int centerX = (int) (totalX / count);
+        int centerZ = (int) (totalZ / count);
+
+        return new ChunkPos(centerX, centerZ);
+    }
+}
