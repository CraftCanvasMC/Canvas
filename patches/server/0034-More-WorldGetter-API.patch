+            ResourceKey.create(net.minecraft.core.registries.Registries.DIMENSION,
+            CraftNamespacedKey.toMinecraft(key))
+        );
+        if (worldServer == null) return null;
+        if (!this.vanillaWorlds.contains(worldServer.getWorld())) return null;
