+            }
+            output = output + "\n    {\n" +
+                "        \"type\": \"{t}\",\n"
+                    .replace("{t}", spawner.mobKey.asString()) +
+                "        \"maxCount\": {t},\n"
+                    .replace("{t}", String.valueOf(spawner.maxCount)) +
+                "        \"minCount\": {t},\n"
+                    .replace("{t}", String.valueOf(spawner.minCount)) +
+                "        \"weight\": {t}\n"
+                    .replace("{t}", String.valueOf(spawner.weight)) +
+                "    }";
+            if(comma){
+                output = output + ",\n";
+            }else{
+                output = output + "\n";
+            }
+        }
+        output = output + "  ]";
+        if(id != "axolotls"){
+            output = output + ",";
+        }
+        output = output + "\n";
+        return output;
+    }
