@@ -9,5 +9,10 @@ public interface Squid extends WaterMob {
      * Sets the EntityType to a Glow Squid.
      */
     public void setGlowingSquid();
+
+    /**
+     * Squirts ink as if the Squid was ingured
+     */
+    public void squirtInk();
     // Canvas end
 }
